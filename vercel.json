{
  "framework": "nextjs",
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "silent": false
  },
  "functions": {
    "api/*.js": {
      "memory": 1024
    },
    "src/app/api/**/*": {
      "memory": 1024
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "/api"
  }
}
